function! Mathon()
    for l in g:math_bindings
        execute "map! " . l[0] . " " . l[1]
    endfor
endfunction

function! Mathoff()
    for l in g:math_bindings
        execute "unmap! " . l[0]
    endfor
endfunction

command! Mon call Mathon()
command! Moff call Mathoff()

